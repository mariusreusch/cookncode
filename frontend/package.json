{
  "name": "cookncode-app",
  "version": "1.0.0",
  "description": "A magnificent app to manage events of coffee machines",
  "scripts": {
    "start": "webpack-dev-server --progress",
    "test": "karma start",
    "test:tdd": "karma start -- --no-single-run",
    "test:pree2e": "yarn run webdriver:update -- --standalone",
    "test:e2e": "yarn run protractor",
    "build": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "clean": "rimraf dist",
    "clean:frontend-test": "rimraf test && yarn run clean:lint",
    "clean:lint": "rimraf tslint-result.xml",
    "clean:all": "yarn run clean && yarn run clean:frontend-test && yarn run clean:lint && rimraf node_modules",
    "lint": "yarn run tslint -- --out tslint-result.xml --force --format checkstyle 'src/**/*.ts'",
    "protractor": "protractor",
    "tslint": "tslint",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "yarn run webdriver-manager start",
    "webdriver:update": "yarn run webdriver-manager update"
  },
  "dependencies": {
    "@angular/common": "2.4.9",
    "@angular/compiler": "2.4.9",
    "@angular/core": "2.4.9",
    "@angular/forms": "2.4.9",
    "@angular/http": "2.4.9",
    "@angular/platform-browser": "2.4.9",
    "@angular/platform-browser-dynamic": "2.4.9",
    "@angular/router": "3.4.9",
    "core-js": "2.4.1",
    "rxjs": "5.2.0",
    "zone.js": "0.7.7"
  },
  "devDependencies": {
    "@types/core-js": "0.9.35",
    "@types/jasmine": "2.5.43",
    "@types/karma-jasmine": "0.0.29",
    "@types/node": "7.0.5",
    "@types/selenium-webdriver": "2.53.40",
    "@types/webpack": "2.2.7",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "3.0.8",
    "codelyzer": "3.0.0-beta.0",
    "css-loader": "0.26.2",
    "extract-text-webpack-plugin": "2.0.0",
    "file-loader": "0.10.1",
    "html-loader": "0.4.5",
    "html-webpack-plugin": "2.28.0",
    "istanbul-instrumenter-loader": "2.0.0",
    "jasmine-core": "2.5.2",
    "jasmine-reporters": "2.2.0",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-jasmine": "1.1.0",
    "karma-junit-reporter": "1.2.0",
    "karma-mocha-reporter": "2.2.2",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "2.0.2",
    "null-loader": "0.1.1",
    "phantomjs": "2.1.7",
    "phantomjs-prebuilt": "2.1.14",
    "protractor": "5.1.1",
    "protractor-jasmine2-html-reporter": "0.0.7",
    "raw-loader": "0.5.1",
    "resolve-url-loader": "2.0.2",
    "rimraf": "2.6.1",
    "script-loader": "0.7.0",
    "source-map-loader": "0.1.6",
    "style-loader": "0.13.2",
    "ts-helpers": "1.1.2",
    "ts-node": "2.1.0",
    "tslint": "4.5.1",
    "tslint-loader": "3.4.3",
    "typescript": "2.2.1",
    "webpack": "2.2.1",
    "webpack-dev-server": "2.4.1",
    "webpack-merge": "3.0.0"
  }
}
